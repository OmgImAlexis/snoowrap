{
  "name": "snoowrap",
  "version": "1.23.0",
  "license": "MIT",
  "description": "A JavaScript wrapper for the reddit API",
  "main": "dist/snoowrap.js",
  "types": "dist/snoowrap.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "eslint",
    "prepare": "npm run build",
    "build": "tsup ./src/snoowrap.ts --dts-resolve",
    "test": "ts-mocha ./test/**/*.spec.ts --timeout 10000",
    "test:coverage": "nyc --reporter=html --reporter=text --reporter=text-summary npm test",
    "type:coverage": "typescript-coverage-report",
    "coverage": "npm run type:coverage && npm run test:coverage",
    "bundlesize": "bundlesize"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/not-an-aardvark/snoowrap.git"
  },
  "keywords": [
    "reddit",
    "api",
    "wrapper",
    "praw",
    "snoo"
  ],
  "author": "not-an-aardvark <not-an-aardvark@users.noreply.github.com>",
  "bugs": {
    "url": "https://github.com/not-an-aardvark/snoowrap/issues"
  },
  "homepage": "https://github.com/not-an-aardvark/snoowrap",
  "dependencies": {
    "bluebird": "^3.7.2",
    "dedent": "^0.7.0",
    "lodash": "^4.17.21",
    "merge-deep": "^3.0.3",
    "p-map": "^5.1.0",
    "p-map-series": "^3.0.0",
    "p-tap": "^4.0.0",
    "promise-chains": "^0.3.11",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "ws": "^3.3.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/lodash": "^4.14.171",
    "@types/merge-deep": "^3.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.2",
    "@types/node-fetch": "^2.5.12",
    "@types/request": "^2.48.6",
    "@types/request-promise": "^4.1.48",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "ava": "^3.15.0",
    "bundlesize": "^0.18.1",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-promise": "^5.1.0",
    "fs-extra": "^8.1.0",
    "ink-docstrap": "^1.2.1",
    "mocha": "^6.0.0",
    "moment": "^2.14.1",
    "node-fetch": "^2.6.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.1.0",
    "tsup": "^4.12.5",
    "typescript": "^4.3.5",
    "typescript-coverage-report": "^0.6.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#1db50c3b169ee4195e1935013d6721628eb5b4bd"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "browser": {
    "fs": false,
    "request-promise": false,
    "util": false,
    "ws": false
  },
  "bundlesize": [
    {
      "path": "./dist/snoowrap.js",
      "maxSize": "20 kB",
      "compression": "brotli"
    },
    {
      "path": "./dist/snoowrap.js",
      "maxSize": "20 kB",
      "compression": "gzip"
    },
    {
      "path": "./dist/snoowrap.js",
      "maxSize": "90 kB",
      "compression": "none"
    }
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  }
}
